# 代理池轮换微服务环境配置文件

# 服务基本配置
MARKET=hk
MODE=live
AUTO_RUN=true

# 网络配置
HOST=0.0.0.0
PORT=8080
RELOAD=false
WORKERS=1

# 代理池配置
POOL_TYPE=memory_ab
ROTATE_INTERVAL_SEC=180
LOW_WATERMARK=5
TARGET_SIZE=20
CHECK_INTERVAL_SEC=60
TERMINATION_GRACE_SEC=10

# 批次获取配置
SINGLE_BATCH_SIZE=200
MIN_REFRESH_SECS=420
BATCH_COUNT=2

# 日志配置
LOG_LEVEL=INFO

# 应用信息
APP_NAME=saturn-proxy-pool
SERVICE_NAME=proxy-pool-service