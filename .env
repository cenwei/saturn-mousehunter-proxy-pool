# 代理池轮换微服务环境配置文件

# 服务基本配置
MARKET=CN
MODE=live
AUTO_RUN=true

# 网络配置
HOST=0.0.0.0
# PORT配置从saturn_mousehunter_shared项目的service_endpoints.py获取
RELOAD=false
WORKERS=1

# 代理池配置
POOL_TYPE=memory_ab
ROTATE_INTERVAL_SEC=180
LOW_WATERMARK=5
TARGET_SIZE=20
CHECK_INTERVAL_SEC=60
TERMINATION_GRACE_SEC=10

# 批次获取配置
SINGLE_BATCH_SIZE=20
MIN_REFRESH_SECS=420
BATCH_COUNT=2

# 海量代理API配置（测试用 num=20）
HAILIANG_API_URL=http://api.hailiangip.com:8422/api/getIp?type=1&num=20&pid=-1&unbindTime=600&cid=-1&orderId=O25062920421786879509&time=1751266950&sign=d758b85241594a8b751147b511b836bf&noDuplicate=1&dataType=0&lineSeparator=0
HAILIANG_ENABLED=true

# 日志配置
LOG_LEVEL=INFO

# 应用信息
APP_NAME=saturn-proxy-pool
SERVICE_NAME=proxy-pool-service